numbers = [1, -5, 3, -8, 5, 0, 7]
start_index = 0
end_index = len(numbers)-1
while start_index < end_index:
    if numbers[start_index] < 0:
        end_index -= 1
        numbers[start_index], numbers[end_index] = numbers[end_index], numbers[start_index]
        print(f"If Number Is Negative ",numbers)
    else:
        numbers[start_index], numbers[end_index] = numbers[end_index], numbers[start_index]
        end_index -= 1
        print("Else Shuffle ",numbers)  
